{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "description": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "source": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "destination": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "startDate": {
      "type": "string",
      "format": "date"
    },
    "endDate": {
       "type": "string",
       "format": "date"
    },
    "fk_organizerid": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["name", "description", "source", "destination", "fk_organizerid"]
}
